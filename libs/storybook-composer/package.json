{
  "name": "@blameable/storybook-composer",
  "version": "0.0.1",
  "main": "src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "nx": {
    "targets": {
      "docker-build": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "docker build --rm -t blameable/storybook -f libs/storybook-composer/Dockerfile ."
          ]
        }
      },
      "storybook": {
        "options": {
          "port": 4403
        }
      },
      "static-storybook": {
        "options": {
          "port": 4403
        }
      }
    }
  }
}
