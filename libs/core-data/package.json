{
  "name": "@blameable/core-data",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "nx": {
    "targets": {
      "better-auth-generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx @better-auth/cli generate -y --config src/lib/auth.ts --output src/lib/drizzle-schemas/auth.ts",
          "cwd": "libs/core-data/src/lib"
        }
      },
      "drizzle-push": {
        "executor": "nx:run-commands",
        "options": {
          "command": "drizzle-kit push",
          "cwd": "libs/core-data"
        }
      },
      "drizzle-migrate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "drizzle-kit migrate",
          "cwd": "libs/core-data"
        }
      },
      "drizzle-generate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "drizzle-kit generate",
          "cwd": "libs/core-data"
        }
      }
    }
  }
}
