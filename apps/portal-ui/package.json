{
  "name": "@blameable/portal-ui",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "docker-build": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "docker build --rm -t blameable/portal-ui -f apps/portal-ui/Dockerfile ."
          ]
        }
      },
      "storybook": {
        "options": {
          "port": 4400
        }
      },
      "static-storybook": {
        "options": {
          "port": 4400
        }
      }
    }
  }
}
